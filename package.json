{
  "name": "typescript-server",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run prod-start",
    "build": "npm run prod-build",
    "deploy": "npm run github-push",
    "test": "npm run jest-test",
    "migration": "npm run typeorm-migration",
    "rollback": "npm run typeorm-rollback",
    "dev-start": "nodemon ./src/expressApp.ts",
    "dev-build": "npm run typescript-build",
    "dist-build": "npm run webpack-build",
    "prod-start": "node ./dist/index.js",
    "prod-build": "",
    "express-server": "nodemon ./src/expressApp.ts",
    "netlify-build": "netlify build",
    "netlify-server": "netlify dev",
    "netlify-deploy": "netlify deploy",
    "netlify-production": "netlify deploy --prod",
    "knex-migration": "knex migrate:latest --env mysql",
    "knex-migration-create": "knex migrate:make my-migration -x ts",
    "knex-seed": "knex seed:run --env mysql",
    "knex-seed-create": "knex seed:make my-seed -x ts",
    "knex-rollback": "knex migrate:rollback --env mysql",
    "mysql-migration": "knex migrate:latest --env mysql",
    "mysql-seed": "knex seed:run --env mysql",
    "mysql-rollback": "knex migrate:rollback --env mysql",
    "elephantsql-migration": "knex migrate:latest --env elephantsql",
    "elephantsql-seed": "knex seed:run --env elephantsql",
    "elephantsql-rollback": "knex migrate:rollback --env elephantsql",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm-migration": "npm run typeorm schema:sync --dataSource data-source.ts",
    "typeorm-seed": "npm run typeorm migration:run --dataSource data-source.ts",
    "typeorm-rollback": "npm run typeorm migration:revert --dataSource data-source.ts",
    "jest-test": "jest",
    "jest-watch": "jest --watchAll",
    "github-push": "git push github staging",
    "github-add": "git remote add github",
    "github-logout": "npm run github-info && git credential reject",
    "github-login": "npm run github-info && git credential fill",
    "github-info": "echo Type protocol=https host=github.com",
    "webpack-build": "webpack --config webpack.config.node.js",
    "typescript-build": "tsc"
  },
  "keywords": [],
